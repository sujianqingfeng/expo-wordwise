{
	"expo": {
		"name": "expo-wordwise",
		"slug": "expo-wordwise",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.wordwise.app"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.wordwise.app"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		},
		"schema": "wordwise",
		"plugins": [
			"expo-router",
			[
				"expo-build-properties",
				{
					"ios": {
						"newArchEnabled": true
					},
					"android": {
						"newArchEnabled": true
					}
				}
			]
		]
	}
}
